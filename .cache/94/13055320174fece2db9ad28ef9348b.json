tes} from \"./tracks.model\";\r\n\r\nexport const getControlDot = (point1: Coordinates, point2: Coordinates) => {\r\n\r\n    const firstPoint = point1.x < point2.x ? point1 : point2\r\n    const secondPoint = point1.x < point2.x ? point2 : point1\r\n\r\n    const controlX = firstPoint.x + (secondPoint.x - firstPoint.x) / 2\r\n\r\n    const controlY = firstPoint.y + (secondPoint.y - firstPoint.y) / 2\r\n\r\n    // const controlPoint = {\r\n    //     x: controlX,\r\n    //     y: controlY\r\n    // }\r\n\r\n    const controlPoint = {\r\n        x: (point1.x + point2.x) / 2,\r\n        y: (point1.y + point2.y) / 2\r\n    }\r\n\r\n\r\n    console.log('point1', point1)\r\n    console.log('point2', point2)\r\n    console.log('controlPoint', controlPoint)\r\n\r\n    return controlPoint\r\n}\r\n\r\n\r\nexport const GetHelpPoint = (b: Coordinates, c: Coordinates) => {\r\n\r\n    const ab = 100\r\n    const bc = Math.sqrt(Math.pow(b.x - c.x, 2) + Math.pow(b.y - c.y, 2));\r\n\r\n    return {\r\n        x: b.x + ab * (c.y - b.y) / bc,\r\n        y: b.y - ab * (c.x - b.x) / bc\r\n    }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"4e1293610ff9e7c628aeda120c420afe","cacheData":{"env":{}}}