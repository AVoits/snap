:null,"original":{"line":28,"column":24},"generated":{"line":55,"column":22}},{"source":"utils/mathCoordinates.ts","name":null,"original":{"line":28,"column":13},"generated":{"line":55,"column":33}}],"sources":{"utils/mathCoordinates.ts":"import {RADIUS, RADIUS_ITEM} from \"./CONST\";\r\n\r\nexport const getRad = (ang: number): number => {\r\n    return (ang * Math.PI) / 180\r\n}\r\n\r\nexport const getCartesian = (p: number, angle: number) => {\r\n    const f = getRad(angle)\r\n    const posX = Math.round(p * Math.cos(f))\r\n    const posY = Math.round(p * Math.sin(f))\r\n    return {posX, posY}\r\n}\r\n\r\n\r\nexport const getPoint = (r: number, angle: number) => {\r\n    const {posX, posY} = getCartesian(r, angle)\r\n    return {\r\n        grad: angle,\r\n        posX,\r\n        posY\r\n    }\r\n}\r\n\r\n\r\n// cos (α) = (b2 + c2 — a2) / 2bc\r\n// расчет минимального угла для третьего круга на основании радиуса фотки\r\n\r\nexport const getMinAngle = () => {\r\n    const side = RADIUS.R3 - RADIUS.R2\r\n    const side2 = side * side\r\n    const ang = (side2 + side2 - RADIUS_ITEM.R2*RADIUS_ITEM.R2)/ 2*side2\r\n\r\n    const RADIAN = Math.acos(ang);\r\n    const GRADS = RADIAN * 180 / Math.PI;\r\n\r\n    return {rad: RADIAN, grad: GRADS}\r\n}"},"lineCount":null}},"error":null,"hash":"37e741f01bbc5b5e3379dd902917d54b","cacheData":{"env":{}}}